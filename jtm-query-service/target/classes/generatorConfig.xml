<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!--导入属性配置 -->
	<properties resource="generator.properties"></properties>
	<classPathEntry location="${jdbc.driverLocation}" />
	<context id="context1">
		<jdbcConnection driverClass="${jdbc.driverClass}"
			connectionURL="${jdbc.connectionURL}" userId="${jdbc.userId}"
			password="${jdbc.password}" />
		<javaModelGenerator targetPackage="${mybatis.model}"
			targetProject="jtm-common/src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="${mybatis.xml}"
			targetProject="jtm-query-service/src/main/resources">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="${mybatis.mapper}"
			targetProject="jtm-query-service/src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		
		<table tableName="lccontother" domainObjectName="LCContOther"></table>
	</context>
</generatorConfiguration>













